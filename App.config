CASE WHEN "AccountLevel"= 4 THEN "AccountNameLevel4" ELSE 
CASE WHEN "AccountLevel"= 5 THEN "AccountNameLevel5" ELSE 
CASE WHEN "AccountLevel"= 6 THEN "AccountNameLevel6" ELSE 
