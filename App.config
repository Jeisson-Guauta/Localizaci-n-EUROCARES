							"U_SaldoFinal", "U_TransType", "U_DocBase", "U_BaseRef", "U_CreatedBy", "U_TransId", "U_CompnyName","U_Street", "U_TaxIdNum", "U_Phone1", "U_FechanCont",
							"U_AccountCodeLevel9", "U_AccountNameLevel9", "U_AccountCodeLevel8", "U_AccountNameLevel8", "U_AccountCodeLevel7", "U_AccountNameLevel7", "U_AccountCodeLevel6", "U_AccountNameLevel6",
							"U_AccountCodeLevel5", "U_AccountNameLevel5", "U_AccountCodeLevel4", "U_AccountNameLevel4", "U_AccountCodeLevel3", "U_AccountNameLevel3", "U_AccountCodeLevel2", "U_AccountNameLevel2",
