      <setting name="idAsientoCanNC" serializeAs="String">
        <value>SELECT T0."TransId",  T0."{0}" 
		FROM OJDT AS T0 
		INNER JOIN JDT1 AS T1 ON T0."TransId"=T1."TransId"
		WHERE T0."TransId" IN(SELECT T2."TransId"
					FROM {1} AS T2 
					WHERE "U_SCL_Cancelar" = '{2}' ) 
					AND "U_SCL_idAsientoNC" IS NULL
		GROUP BY T0."TransId",  T0."{0}"</value>
      </setting>
      <setting name="DocEntryNC" serializeAs="String">
        <value>SELECT "DocEntry", "CardCode" FROM {0} WHERE "TransId" = '{1}'</value>
      </setting>
      <setting name="Cierre" serializeAs="String">
        <value>SELECT T1."ShortName" as "CardCode",'' as "CardName", SUM(T1."Debit" - T1."Credit") AS "Balance", T1."Account" as "DebPayAcct",
'' as "Incluir"
FROM OJDT T0
INNER JOIN JDT1 T1 ON T0."TransId" = T1."TransId"
WHERE T0."RefDate" BETWEEN '{0}' AND '{1}'
GROUP BY T1."Account", T1."ShortName"
Having SUM(T1."Debit" - T1."Credit") &gt; 0</value>
      </setting>
      <setting name="TipoDocumento" serializeAs="String">
        <value>SELECT DISTINCT "TransType", CASE  WHEN "TransType" ='13' AND "DocSubType" = 'DN' THEN 'ND Clientes' 
											  WHEN "TransType" ='13' AND "DocSubType" = CONCAT('-','-') THEN 'Fac. Clientes'
											  WHEN "TransType" ='14' THEN 'NC Clientes'
											  WHEN "TransType" ='1470000049' THEN 'Capitalización'
											  WHEN "TransType" ='1470000060' THEN 'NC Capitalización'
											  WHEN "TransType" ='1470000071' THEN 'Eje. Amortizacion'
											  WHEN "TransType" ='1470000085' THEN 'Rev. Activos'
											  WHEN "TransType" ='15' THEN 'Entrega'
											  WHEN "TransType" ='16' THEN 'Devolución'
											  WHEN "TransType" ='162' THEN 'Rev. Inventario'
											  WHEN "TransType" ='18' AND "DocSubType" = CONCAT('-','-') THEN 'Fac. Proveedores' 
											  WHEN "TransType" ='18' AND "DocSubType" = 'DM' THEN 'ND Proveedores'
											  WHEN "TransType" ='19' THEN 'NC Proveedores'
											  WHEN "TransType" ='20' THEN 'Ent. Mercancías'
											  WHEN "TransType" ='21' THEN 'Dev. Mercancías'
											  WHEN "TransType" ='25' THEN 'Deposito'
											  WHEN "TransType" ='202' THEN 'Orden Fabricación'
											  WHEN "TransType" ='203' THEN 'Fac. Ant clientes'
											  WHEN "TransType" ='204' THEN 'Fac. Ant proveedores'
												WHEN "TransType" ='321' THEN 'Reconc. Internas'
											  WHEN "TransType" ='24' THEN 'Pagos recibidos'
											  WHEN "TransType" ='30' THEN 'Asiento'
											  WHEN "TransType" ='310000001' THEN 'Saldo Aper inventario'
											  WHEN "TransType" ='46' THEN 'Pagos efectuados'
											  WHEN "TransType" ='59' THEN 'Ent. Mercancías (Stock)'
											  WHEN "TransType" ='60' THEN 'Emisión Prod.'
											  WHEN "TransType" ='67' THEN 'Transferencia de Stock'
											  WHEN "TransType" ='69' THEN 'Precio Entrega'
											  WHEN "TransType" ='-2' THEN 'Saldo Inicial' 
											  WHEN "TransType" ='-3' THEN 'Saldo Final' 
											  	END AS "Tipo Doc"
											  	 FROM JDT1 ORDER BY 1
</value>
      </setting>
      <setting name="ListaCntas" serializeAs="String">
