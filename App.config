					Left Join OSLP A8 on A0."SlpCode" = A8."SlpCode"
					Where A0."DocEntry" = {0}
					Union All
					Select 2 as "OrdenPlano", 0 as "LineNum", Cast(
					'[@@CRLF]' || Case When A1."VisOrder" = 0 Then '¬' Else '' End ||
					Case When A0."DocType" = 'S' Then IfNull(A1."Dscription",'') Else IfNull(A2."ItemName",'') End || '|' ||
					Cast(Cast(A1."Quantity"  as Decimal(15,2)) as Nvarchar(100)) || '|' || 'GRM' || '|' || Cast(Cast(A1."PriceAfVAT" as Decimal(15,2)) as Nvarchar(100)) || '|' ||
					Cast(Cast(A1."PriceBefDi"*A1."Quantity" as Decimal(15,2)) as Nvarchar(100)) || '|' || '' || '|' || '' || '|' || '' || '|' || 
					'' || '|' || '' || '|' || Cast(IfNull(A1."Text",'') as Nvarchar(100)) || '|' || '' || '|' || '' || '|' || '' || '|' || IfNull(A1."CodeBars",'') || '|' ||
					Case When A0."DocType" = 'S' Then A1."AcctCode" Else A1."ItemCode" end || '|' || '' || '|' || 
					Cast(Cast(IfNull(A1."DiscPrcnt",0)  as Decimal(15,2)) as Nvarchar(100)) || '|' || 
					Cast(Cast((IfNull(A1."DiscPrcnt",0) * A1."PriceBefDi") / 100  as Decimal(15,2)) as Nvarchar(100)) || '|' as text),
					Cast(
					Cast(Cast(A1."PriceBefDi" as Decimal(15,2)) as Nvarchar(100)) || '|' || Cast(Cast(IfNull(A2."NumInSale",0) as Decimal(15,2)) as Nvarchar(100)) ||
					'|' || '' || '|' || Cast(Cast(A1."VatPrcnt" as Decimal(15,2)) as Nvarchar(100)) || '|' || Cast(Cast(IfNull(A1."VatSum",0) as Decimal(15,2)) as Nvarchar(100))
					|| '|'  || Cast(Cast(IfNull(A3."TaxRate",0) as Decimal(15,2)) as Nvarchar(100)) || '|' || Cast(Cast(IfNull(A3."TaxSum",0) as Decimal(15,2)) as Nvarchar(100))
					|| '|'  || Cast(Cast(IfNull(A1."Price",0) as Decimal(15,2)) as Nvarchar(100)) || '|' || Cast(Cast(IfNull(A1."LineTotal",0) as Decimal(15,2)) as Nvarchar(100))
					|| '||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||' || IfNull(A4."BatchNum",'') || '|' || 
					Case When IfNull(A4."BatchNum",'') = '' Then '' Else TO_NVARCHAR(A5."InDate", 'YYYY-MM-DD') End || '||||||||||' as Text)
