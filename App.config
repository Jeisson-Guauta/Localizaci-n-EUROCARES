					ELSE Cast(Cast(Sum(A1."WTAmntSC") as Decimal(15,2)) as Nvarchar(100)) END || '|' ||
					Cast(Cast(A1."Rate" as Decimal(15,2)) as Nvarchar(100)) as "cadena"
					From OINV A0
					Inner Join INV5 A1 On A0."DocEntry" = A1."AbsEntry"
					Inner Join OWHT A2 On A1."WTCode" = A2."WTCode"
					Where A0."DocEntry" = {0}
					Group By A0."DocCur", A1."Rate"
