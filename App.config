      <setting name="Articulos_Series" serializeAs="String">
        <value>SELECT DISTINCT 
	T1."ItemCode",
	CASE WHEN T6."ManSerNum" = 'Y' THEN T10."DistNumber" ELSE '' END AS "Series", 
	CASE WHEN T6."ManBtchNum" = 'Y' THEN T12."BatchNum" WHEN T6."ManSerNum" = 'Y' THEN T10."LotNumber" ELSE '' END AS "Lotes",
	CASE WHEN T6."ManBtchNum" = 'Y' THEN T12."ExpDate" WHEN T6."ManSerNum" = 'Y' THEN T10."ExpDate" ELSE '' END AS "Fecha Ven.",
	T1."LineNum",
	T1."Quantity",
	T1."StockPrice",
	T1."WhsCode",
	T1."OcrCode",
	T1."OcrCode2"
	FROM OWTR T0
	INNER JOIN WTR1 T1 ON T0."DocEntry" = T1."DocEntry" 
	INNER JOIN OITM T6 ON T1."ItemCode" = T6."ItemCode" 
	LEFT JOIN OITL T8 ON T8."DocEntry" = T0."DocEntry" AND T8."DocType" = T0."ObjType" AND T8."DocLine" = T1."LineNum"
	LEFT JOIN ITL1 T9 ON T8."LogEntry" = T9."LogEntry"
	LEFT JOIN OSRN T10 ON T9."MdAbsEntry" = T10."AbsEntry"
	LEFT JOIN IBT1 T11 ON T11."BaseEntry" = T0."DocEntry" AND T11."ItemCode" = T1."ItemCode"
	LEFT JOIN OIBT T12 ON T12."BatchNum"=T11."BatchNum" 
	WHERE T0."DocEntry"  = '{0}' ORDER BY 5 ASC
</value>
      </setting>
      <setting name="QueryAutorret" serializeAs="String">
        <value>
          SELECT TB01."DocEntry" AS "DocEntry",
          CASE TB01."DocSubType" WHEN '--' THEN 'FV' WHEN 'DN' THEN 'ND-V' END AS "Tipo",
          TB01."DocNum" AS "Número",
          TB01."CardCode" AS SN,
          TB02."CardName" AS "Nombre",
          (SELECT SUM(TB06."LineTotal") FROM INV1 TB06 WHERE TB06."WtLiable" = 'Y' AND TB06."DocEntry" = TB01."DocEntry") AS "Base",
          TB04."WTCode" AS "Retencion",
          TB04."PrctBsAmnt" AS "Porcentaje",
          ((SELECT SUM(TB06."LineTotal") FROM INV1 TB06 WHERE TB06."WtLiable" = 'Y' AND TB06."DocEntry" = TB01."DocEntry")*(IFNULL(TB04."PrctBsAmnt",0))/100) AS "Total",
          'Y' AS "Seleccionar"
          FROM OINV AS TB01
          INNER JOIN OCRD AS TB02 ON TB02."CardCode" = TB01."CardCode"
          INNER JOIN CRD4 AS TB03 ON TB03."CardCode" = TB02."CardCode"
          INNER JOIN OWHT AS TB04 ON TB04."WTCode" = TB03."WTCode"
          INNER JOIN NNM1 T2 ON TB01."Series" = T2."Series"
          WHERE TB01."DocDate" BETWEEN CAST('%T1%' AS timestamp) AND CAST('%T2%' AS timestamp)
          AND (IFNULL(TB01."U_SCL_ChkDev", 'N') != 'Y')
          AND TB01.CANCELED = 'N'
          %IFRS_1%
          %CARDCODE%
          %SERIE%
          %DOC_NUM%
          %RETENCIONES%
          UNION ALL
          SELECT TB01."DocEntry" AS "DocEntry",
          'NC-V' AS "Tipo",
          TB01."DocNum" AS "Número",
          TB01."CardCode" AS SN,
          TB02."CardName" AS "Nombre",
          TB01."BaseAmnt" AS "Base",
          TB04."WTCode" AS "Retencion",
          IFNULL(TB04."PrctBsAmnt",0) AS "Porcentaje",
          (((TB01."DocTotal" - TB01."VatSum" + TB01."WTSum" - TB01."TotalExpns")*IFNULL(TB04."PrctBsAmnt",0))/100)  AS "Total",
          'Y' AS "Seleccionar"
          FROM ORIN AS TB01
          INNER JOIN OCRD AS TB02 ON TB02."CardCode" = TB01."CardCode"
          INNER JOIN CRD4 AS TB03 ON TB03."CardCode" = TB02."CardCode"
          INNER JOIN OWHT AS TB04 ON TB04."WTCode" = TB03."WTCode"
          INNER JOIN NNM1 T2 ON TB01."Series" = T2."Series"
          WHERE TB01."DocDate" BETWEEN CAST('%T1%' AS timestamp) AND CAST('%T2%' AS timestamp)
          AND (IFNULL(TB01."U_SCL_ChkDev", 'N') != 'Y')
          AND TB01.CANCELED = 'N'
          %IFRS_1%
          %CARDCODE%
          %SERIE%
          %DOC_NUM%
          %RETENCIONES%
        </value>
      </setting>
      <setting name="QueryAutorretSN" serializeAs="String">
        <value>
          SELECT 'Agrup' AS "Tipo",
          TA01."SN", TA01."Nombre",
          SUM(TA01."Base") as "Base",
          TA01."Retencion",
          TA01."Porcentaje",
          SUM(TA01."Total") AS "Total",
          TA01."Seleccionar"
          FROM
          (
          SELECT TB01."CardCode" AS SN,
          TB02."CardName" AS "Nombre",
          SUM((TB01."DocTotal" - TB01."VatSum" + TB01."WTSum" + TB01."DiscSum" - TB01."TotalExpns")) AS "Base",
          TB04."WTCode" AS "Retencion",
          (SELECT IFNULL(TA01."PrctBsAmnt",0) FROM OWHT AS TA01 WHERE TA01."WTCode" = TB04."WTCode") AS "Porcentaje",
          (SUM((TB01."DocTotal" - TB01."VatSum" + TB01."WTSum" - TB01."TotalExpns")) *
          (SELECT IFNULL(TA01."PrctBsAmnt",0) FROM OWHT AS TA01 WHERE TA01."WTCode" = TB04."WTCode"))/100 AS "Total",'Y' AS "Seleccionar"
          FROM OINV AS TB01
          INNER JOIN OCRD AS TB02 ON TB02."CardCode" = TB01."CardCode"
          INNER JOIN CRD4 AS TB03 ON TB03."CardCode" = TB02."CardCode"
          INNER JOIN OWHT AS TB04 ON TB04."WTCode" = TB03."WTCode"
          INNER JOIN NNM1 T2 ON TB01."Series" = T2."Series"
          WHERE TB01."DocDate" BETWEEN CAST('%T1%' AS timestamp) AND CAST('%T2%' AS timestamp)
          AND (IFNULL(TB01."U_SCL_ChkDev", 'N') != 'Y')
          AND TB01.CANCELED = 'N'
          %IFRS_1%
          %CARDCODE%
          %SERIE%
          %DOC_NUM%
          %RETENCIONES%
          GROUP BY TB01."CardCode", TB02."CardName", TB04."WTCode"

          UNION ALL

          SELECT TB01."CardCode" AS SN,
          TB02."CardName" AS "Nombre",
          (SUM((TB01."DocTotal" - TB01."VatSum" + TB01."WTSum" + TB01."DiscSum" - TB01."TotalExpns")) * -1) AS "Base",
          TB04."WTCode" AS "Retencion",
          (SELECT IFNULL(TA01."PrctBsAmnt",0) FROM OWHT AS TA01 WHERE TA01."WTCode" = TB04."WTCode") AS "Porcentaje",
          ((SUM((TB01."DocTotal" - TB01."VatSum" + TB01."WTSum" - TB01."TotalExpns"))*(SELECT IFNULL(TA01."PrctBsAmnt",0) FROM OWHT AS TA01 WHERE TA01."WTCode" = TB04."WTCode"))/100) * -1 AS "Total",
          'Y' AS "Seleccionar"
          FROM ORIN AS TB01
          INNER JOIN OCRD AS TB02 ON TB02."CardCode" = TB01."CardCode"
          INNER JOIN CRD4 AS TB03 ON TB03."CardCode" = TB02."CardCode"
          INNER JOIN OWHT AS TB04 ON TB04."WTCode" = TB03."WTCode"
          INNER JOIN NNM1 T2 ON TB01."Series" = T2."Series"
          WHERE TB01."DocDate" BETWEEN CAST('%T1%' AS timestamp) AND CAST('%T2%' AS timestamp)
          AND (IFNULL(TB01."U_SCL_ChkDev", 'N') != 'Y')
          AND TB01.CANCELED = 'N'
          %IFRS_1%
          %CARDCODE%
          %SERIE%
          %DOC_NUM%
          %RETENCIONES%
          GROUP BY TB01."CardCode", TB02."CardName", TB04."WTCode"
          ) AS TA01
          GROUP BY TA01.SN, TA01."Nombre", TA01."Porcentaje", TA01."Seleccionar", TA01."Retencion"
        </value>
      </setting>
    </LocalizacionColombia.Consultas>
