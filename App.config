					Left Join OITM A2 On A1."ItemCode" = A2."ItemCode"
					Left Join INV4 A3 On A1."DocEntry" = A3."DocEntry" And A1."LineNum" = A3."LineNum" and A3."ExpnsCode" != '-1'
					Left Join IBT1 A4 On A1."DocEntry" = A4."BaseEntry" And A0."ObjType" = A4."BaseType" And A1."LineNum" = A4."BaseLinNum"
					Left Join OBTN A5 On A4."BatchNum" = A5."DistNumber" And A4."ItemCode" = A5."ItemCode"
					Where A0."DocEntry" = {0}
					Union All
					Select 3 as "OrdenPlano", B0."LineNum" as "LineNum", Cast(
					'[@@CRLF]' || Case When B0."LineNum" = 1 Then '¬' Else '' End as text), Cast(B0."cadena" as Text) From(
					Select ROW_NUMBER() OVER (ORDER BY A1."TaxRate") as "LineNum", 'TR|01|' || 
					CASE WHEN A0."DocCur" in ('$','us', 'eu', 'lb','COP') THEN Cast(Cast(Sum(A1."TaxSum") as Decimal(15,2)) as Nvarchar(100)) 
					WHEN A0."DocCur" = 'usd' THEN Cast(Cast(Sum(A1."TaxSumSys") as Decimal(15,2)) as Nvarchar(100)) 
					ELSE Cast(Cast(Sum(A1."TaxSumFrgn") as Decimal(15,2)) as Nvarchar(100)) END  || '|' ||
					Cast(Cast(A1."TaxRate" as Decimal(15,2)) as Nvarchar(100)) as "cadena" 
					From OINV A0
					Inner Join INV4 A1 On A0."DocEntry" = A1."DocEntry"
