					Cast(Cast(A0."DocTotal" - A0."DiscSum" as Decimal(15,2)) as Nvarchar(100)) || '|' || 'dd85db55545bd6566f36b0fd3be9fd8555c36e' ||
					'|' || '' || '|' || '42' || '|' || '2' || '|' || '|' as Text) as "Cadena2"
