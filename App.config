      <setting name="FormatosMM" serializeAs="String">
        <value>SELECT "Code", "U_SCL_DesFrmtoMM" AS "Descripción" FROM "@SCL_MMFORMATOS" ORDER BY 1</value>
      </setting>
      <setting name="ConceptosMM" serializeAs="String">
        <value>SELECT "Code", "U_SCL_DesCncptoMM" AS "Descripción" FROM "@SCL_MMCONCEPTOS" WHERE "U_SCL_ApFrmtoMM" = '{0}' ORDER BY 1</value>
      </setting>
      <setting name="CuentasMM" serializeAs="String">
        <value>SELECT '' AS "Seleccionar", "AcctCode" AS "Cod Cuenta", "AcctName" AS "Descripcion Cuenta" FROM OACT WHERE "Levels" = '5' ORDER BY 2</value>
      </setting>
      <setting name="ParamCntasMM" serializeAs="String">
        <value>INSERT INTO "@SCL_PARAMCNTASMM" ("Code", "Name","U_SCL_FrmtoMM", "U_SCL_CncptoMM", "U_SCL_CodigoCnta", "U_SCL_NombreCnta") VALUES ('{0}', '{1}','{2}','{3}', '{4}', '{5}');</value>
      </setting>
      <setting name="CntasCncptosMM" serializeAs="String">
        <value>SELECT "U_SCL_CodigoCnta" AS "Cod Cuenta", "U_SCL_NombreCnta" AS "Descripcion", "U_SCL_TipoMov" AS "Tipo Movimiento", "U_SCL_TipoRet" AS "Tipo Retencion", '' AS "Seleccionar" FROM "@SCL_PARAMCNTASMM" WHERE "U_SCL_FrmtoMM" = '{0}' AND "U_SCL_CncptoMM" = '{1}' ORDER BY 1</value>
      </setting>
      <setting name="TipoMovimientoMM" serializeAs="String">
        <value>SELECT "Code", "Name" FROM "@SCL_MMTIPOMOV"</value>
      </setting>
      <setting name="TipoRetencionMM" serializeAs="String">
        <value>SELECT "Code", "Name" FROM "@SCL_MMTIPORET"</value>
      </setting>
      <setting name="ActPrmCntsMM" serializeAs="String">
        <value>UPDATE "@SCL_PARAMCNTASMM"
SET "U_SCL_TipoMov" = '{0}', "U_SCL_TipoRet" = '{1}' WHERE "U_SCL_FrmtoMM"='{2}' AND "U_SCL_CncptoMM" = '{3}' AND "U_SCL_CodigoCnta" = '{4}'</value>
      </setting>
      <setting name="ElimCntsMM" serializeAs="String">
        <value>DELETE FROM "@SCL_PARAMCNTASMM" WHERE "U_SCL_CodigoCnta" = '{0}'</value>
      </setting>
      <setting name="GenMM" serializeAs="String">
        <value>SELECT 
T5."U_SCL_DesCncptoMM" AS "Concepto",
T2."U_SCL_TipoDoc" AS "Tipo documento",
T2."U_SCL_Identificacion" AS "Número identificación",
RIGHT(T2."LicTradNum", 1) AS "DV",
T2."U_SCL_Apellido1" AS "Primer apellido", 
T2."U_SCL_Apellido2" AS "Segundo apellido", 
T2."U_SCL_Nombre1" AS "Primer nombre", 
T2."U_SCL_NombresAdici" AS "Otros nombres", 
T2."U_SCL_RazonSocial" AS "Razón social",
T3."Street" AS "Dirección",
T2."U_SCL_CodigoDpto" AS "Código Dpto",
T2."U_SCL_CodigoMun" AS "Código Mcp",
T2."U_SCL_PaisDom" AS "País de residencia",
T2."U_SCL_TipoPersona" AS "Tipo persona", 
T2."U_SCL_RegTributario" AS "Regimén tributario", 
T1."Account" AS "Código Cuenta",
T0."TransId" AS "Num Asiento",
T1."Debit",
T1."Credit",
T8."FinancialPeriodClosingBalanceLC" AS "Saldo"
FROM OJDT T0 
INNER JOIN JDT1 T1 ON T0."TransId" = T1."TransId" 
INNER JOIN OCRD T2 ON T2."CardCode" = T1."U_SCL_CodeSN"
LEFT JOIN CRD1 T3 ON T3."CardCode" = T1."U_SCL_CodeSN"
INNER JOIN "@SCL_PARAMCNTASMM" T4 ON T4."U_SCL_CodigoCnta" = T1."Account"
INNER JOIN "@SCL_MMCONCEPTOS" T5 ON T5."Code" = T4."U_SCL_CncptoMM"
INNER JOIN OACT T6 ON T6."AcctCode" = T1."Account"
LEFT JOIN OADM T7 ON 1=1
LEFT JOIN (SELECT "AccountCode", "FinancialPeriodClosingBalanceLC", "FinancialPeriodClosingBalanceSC" FROM "_SYS_BIC"."sap.bluetabcb00.fin.fi/BalanceSheetComparisonQuery"('PLACEHOLDER' = ('$$P_FinancialPeriod$$',
'{0}'))) T8 ON T8."AccountCode" = T1."Account" 
WHERE T1."Account" IN ({1}) 
AND T0."RefDate" &gt;= '{2}' 
AND T0."RefDate" &lt;= '{3}'
ORDER BY T1."Account"
</value>
